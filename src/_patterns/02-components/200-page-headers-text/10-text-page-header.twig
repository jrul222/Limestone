{% embed 'molecules-slab' with {slab_variant : slab_skin ~ ' page-header page-header--text'} %}
    {% block slab_inner %}
        {% include 'molecules-headline-group' with { level: 1, head:fake('words',{nb: 3 + random(10), asText: true})|titleize } %}
        {% if with_cta %}
            {% include 'molecules-cta-group' with {cta_type: 'mixed'} %}
        {% endif %}
    {% endblock %}
{% endembed %}
