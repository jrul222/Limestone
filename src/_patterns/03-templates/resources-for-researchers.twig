{% include 'organisms-main-header' %}

{% include 'organisms-page-header' with {
	title: "Resources for Researchers",
	tagline: true
} %}

{% embed 'molecules-page-section' %}

	{% block page_section_content %}

		{% include 'organisms-callout' with {
			icon: 'megaphone',
			action_link: true,
			callout_style_modifier: 'dark wide-layout'
			}
		%}

		{% include 'organisms-accordion' %}

	{% endblock %}


	{% block page_section_sidebar %}

		{% include 'organisms-announcements' with {
			announcements_modifier: 'narrow'
		} %}



		{% include 'organisms-events-list' with {
			events_list_style_modifier: 'narrow',
			events_superhead: 'Research',
			events_headline: 'Events',
			buttons: [
				{
					button_text: 'All Events',
					button_style: 'accent'
				}]
		} %}

	{% endblock %}

{% endembed %}





{% include 'organisms-footer' %}