{% extends 'base.twig' %}
{% block main_content %}
	{% include 'organisms-page-header' with { headline : 'Generic Page', superhead : false, subhead : 'Where 90% of the content goes' } %}

	{% embed 'molecules-page-section' %}
		{% block page_section_content %}
			{% include 'organisms-panel-media' %}
		{% endblock %}
	{% endembed %}

	{% embed 'molecules-page-section' %}
		{% block page_section_content %}
			{% embed 'molecules-with-sidebar' %}
				{% block sidebar_content %}
					{% include 'organisms-editorial-panel' %}
				{% endblock %}
				{% block sidebar_sidebars %}
					{% include 'organisms-media-aside' %}
					{% include 'organisms-teaser-aside' %}
					{% include 'organisms-pullquote-aside' %}
				{% endblock %}
			{% endembed %}
		{% endblock %}
	{% endembed %}

	{% embed 'molecules-page-section' with { page_section_style : 'page-section--dark' } %}
		{% block page_section_content %}
			{% include 'organisms-panel-media' %}
		{% endblock %}
	{% endembed %}

{% endblock %}
