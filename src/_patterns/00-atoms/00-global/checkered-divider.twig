<div class="text-center checkered-divider">
  <svg id="blueCheckered" width="100%" height="100px" xmlns="http://www.w3.org/2000/svg">
    <rect stroke="{{ color }}" id="svg_2" height="20px" width="20px" y="5px" x="calc(50% - 40px)" stroke-width="1" fill="none"/>
    <rect stroke="{{ color }}" id="svg_3" height="20px" width="20px" y="25px" x="calc(50% - 20px)" stroke-width="1" fill="none"/>
    <rect stroke="{{ color }}" id="svg_4" height="20px" width="20px" y="5px" x="calc(50% + 0px)" stroke-width="1" fill="none"/>
    <rect stroke="{{ color }}" id="svg_5" height="20px" width="20px" y="25px" x="calc(50% + 20px)" stroke-width="1" fill="none"/>
    <line stroke-linecap="null" stroke-linejoin="null" id="svg_6" y2="25px" x2="100%" y1="25px" x1="0" stroke-width="1" stroke="{{ color }}" fill="none"/>
  </svg>
</div>
